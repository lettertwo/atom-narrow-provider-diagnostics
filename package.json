{
  "name": "atom-narrow-provider-diagnostics",
  "version": "0.1.0",
  "description": "Narrow nuclide diagnostics (linter, flow, etc)",
  "main": "./lib/main",
  "keywords": [
    "narrow",
    "nuclide",
    "diagnostics",
    "linter",
    "flow"
  ],
  "repository": "https://github.com/lettertwo/atom-narrow-provider-diagnostics",
  "author": "Eric Eldredge <lettertwo@gmail.com>",
  "license": "MIT",
  "activationCommands": {
    "atom-text-editor": [
      "narrow:diagnostics"
    ]
  },
  "engines": {
    "atom": ">=0.174.0 <2.0.0"
  },
  "consumedServices": {
    "narrow": {
      "versions": {
        "1.0.0": "consumeNarrow"
      }
    },
    "atom-ide-diagnostics-store": {
      "versions": {
        "0.2.0": "consumeObservableDiagnosticUpdates"
      }
    }
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^2.1.1",
    "eslint-plugin-prettier": "^2.1.1",
    "prettier": "^1.3.1"
  }
}
